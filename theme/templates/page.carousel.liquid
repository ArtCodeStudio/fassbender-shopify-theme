<div class="container pt-5">
  <div class="row">
    <div class="col-xs-12">
      {% include 'jumplink-carousel', carousel_handle: page.handle %}
    </div>
    <div class="col-xs-12 text-center">
      <h1 class="page-title mt-0 mb-4">{{ page.title }}</h1>
    </div>

    {% unless page.content == blank %}
        <div class="col-xs-12 col-md-8 offset-md-2 rte">
        {{ page.content }}
        </div>
    {% endunless %}
    
    {% include 'jumplink-register-box' %}

  </div>
</div>