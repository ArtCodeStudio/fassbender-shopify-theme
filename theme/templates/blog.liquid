{% paginate blog.articles by 10 %}
  <router-view id="blog-pagination" listen-all-links="false" dataset-to-model="false" parse-title="false" scroll-to-top="false" prefetch-links="true" action="append">
    {% for article in blog.articles %}
      {% include 'jumplink-article-list-item' %}
    {% endfor %}
    {% include 'jumplink-pagination' with viewId: 'blog-pagination' %}
  </router-view>
{% endpaginate %}
