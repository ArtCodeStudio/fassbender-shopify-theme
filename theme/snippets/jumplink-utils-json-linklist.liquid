{{linklist.handle | json}}: {
    {% if linklist.handle %}
        "handle": {{linklist.handle | json}},
    {% endif %}
    {% if linklist.id %}
        "id": {{linklist.id | json}},
    {% endif %}
    {% if linklist.links %}
        "links": {% include "jumplink-utils-json-links", links: linklist.links %},
    {% endif %}
    {% if linklist.title %}
        "title": {{linklist.title | json}}
    {% endif %}
}