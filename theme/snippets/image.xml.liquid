<image_xml class="hidden">
    <alt>
    	{{image.alt}}
    </alt>
    <attached_to_variant>
    	{{image.attached_to_variant?}}
    </attached_to_variant>
    <id>
    	{{image.id}}
    </id>
    <product_id>
    	{{image.product_id}}
    </product_id>
    <position>
    	{{image.position}}
    </position>
    <src>
    	{{image.src}}
    </src>
    {% if image.variants.size > 0 %}
        <variants>
            {% comment %} TODO {% endcomment %}
        </variants>
    {% endif %}
</image_xml>