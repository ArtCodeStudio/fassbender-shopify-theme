{% comment %}
Is the current collection unlocked?
{% endcomment %}
{% assign unlocked = false %}
{% if customer %}
    {% assign collectionHandles = customer.metafields.c_f.jumplink_unlocked_collections | strip | remove: ' ' | split: ',' %}
    {% for collectionHandle in collectionHandles %}
        {% if collection.handle == collectionHandle %}
            {% assign unlocked = true %}
        {% endif %}
    {% endfor %}
{% endif %}