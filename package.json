{
  "name": "fassbender-shopify-theme",
  "version": "2.7.8",
  "description": "Shopify theme based on Bootstrap 4",
  "main": "src/ts/main.ts",
  "typings": "src/ts/types",
  "scripts": {
    "theme:download": "theme download --dir './theme'",
    "theme:download:configs": "theme download --dir './theme' locales config",
    "theme:download:locales": "theme download --dir './theme' locales",
    "theme:download:config": "theme download --dir './theme' config",
    "webpack": "webpack --env production",
    "webpack:dev": "webpack --env development",
    "deploy": "npm run build && theme deploy --dir ./theme",
    "deploy:webpack": "npm run build && theme deploy --dir ./theme assets/main.bundle.js assets/vendors.bundle.js assets/main.css",
    "lint": "eslint ./src --ext .ts,.tsx --fix && tsc --noEmit",
    "test": "npm run lint",
    "build": "npm run lint && npm run webpack",
    "manual:build:prod:sass": "node-sass --output-style compressed src/scss/main.scss -o theme/assets/ && postcss -c postcss.config.js theme/assets/main.css -o theme/assets/main.css",
    "build:dev": "npm run lint && npm run webpack:dev",
    "zip": "gulp build:zip",
    "watch": "concurrently \"npm:watch:dev*\"",
    "watch:dev:theme": "theme watch --dir './theme'",
    "watch:dev:webpack": "npm run webpack:dev -- --watch",
    "watch:prod": "concurrently \"npm:watch:prod:*\"",
    "watch:prod:theme": "theme watch --dir './theme'",
    "watch:prod:webpack": "npm run webpack -- --watch",
    "checkout": "concurrently -k \"npm:checkout:*\"",
    "checkout:js": "uglifyjs checkout/custom-checkout.js -o checkout/custom-checkout.min.js -c -m;",
    "checkout:css": "cleancss checkout/custom-checkout.css -o checkout/custom-checkout.min.css",
    "packages:upgrade": "yarn upgrade-interactive"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ArtCodeStudio/fassbender-shopify-theme.git"
  },
  "keywords": [
    "Art+Code Studio",
    "Bootstrap4",
    "Shopify",
    "myfassbender",
    "Fassbender"
  ],
  "author": "Pascal Garber <pascal@jumplink.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArtCodeStudio/fassbender-shopify-theme/issues"
  },
  "homepage": "https://art-code-studio.myshopify.com",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.23.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-syntax-export-default-from": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.23.9",
    "@babel/runtime-corejs3": "^7.23.9",
    "@ribajs/eslint-config": "^2.0.0-rc.10",
    "@ribajs/tsconfig": "^2.0.0-rc.10",
    "@ribajs/webpack-config": "^2.0.0-rc.10",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@yarnpkg/pnpify": "^4.0.1",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.1.3",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "clean-css-cli": "^5.6.3",
    "concurrently": "^8.2.2",
    "core-js": "^3.35.1",
    "css-flip": "^0.5.0",
    "css-loader": "^6.9.1",
    "csso-cli": "^4.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "extract-loader": "^5.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-jsoncombine": "^1.0.4",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^6.0.0",
    "html-loader": "^5.0.0",
    "inspectpack": "^4.7.1",
