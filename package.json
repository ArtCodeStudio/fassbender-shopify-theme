{
  "name": "fassbender-shopify-theme",
  "version": "2.1.0",
  "description": "Shopify theme based on Bootstrap 4",
  "main": "src/ts/main.ts",
  "typings": "src/ts/types",
  "scripts": {
    "theme:download": "theme download --dir './theme'",
    "theme:download:locales": "theme download --dir './theme' locales",
    "theme:download:config": "theme download --dir './theme' config",
    "webpack": "webpack --env.production",
    "webpack:dev": "webpack --env.development",
    "deploy": "npm run build && theme deploy --dir ./theme",
    "deploy:webpack": "npm run build && theme deploy --dir ./theme assets/main.bundle.js assets/vendors.main.bundle.js",
    "deploy:sass": "npm run manual:build:prod:sass && theme deploy --dir ./theme assets/bundle.css",
    "format:ts": "eslint ./src --ext .ts,.tsx --fix && tsc --noEmit",
    "test": "npm run format:ts",
    "build": "npm run format:ts && npm run webpack",
    "manual:build:prod:sass": "node-sass --output-style compressed src/scss/main.scss -o theme/assets/ && postcss -c postcss.config.js theme/assets/main.css -o theme/assets/main.css",
    "build:dev": "npm run format:ts && npm run webpack:dev",
    "zip": "gulp build:zip",
    "watch": "concurrently \"npm:watch:dev*\"",
    "watch:dev:theme": "theme watch --dir './theme'",
    "watch:dev:webpack": "npm run webpack:dev -- --watch",
    "manual:watch:sass": "nodemon -e scss -x 'npm run build:prod:sass'",
    "watch:prod": "concurrently \"npm:watch:prod:*\"",
    "watch:prod:theme": "theme watch --dir './theme'",
    "watch:prod:webpack": "npm run webpack -- --watch",
    "checkout": "concurrently -k \"npm:checkout:*\"",
    "checkout:js": "uglifyjs checkout/custom-checkout.js -o checkout/custom-checkout.min.js -c -m;",
    "checkout:css": "cleancss checkout/custom-checkout.css -o checkout/custom-checkout.min.css",
    "packages:npm:link": "npm link @ribajs/core @ribajs/i18n @ribajs/extras @ribajs/jquery @ribajs/bs4 @ribajs/shopify @ribajs/router @ribajs/shopify-tda",
    "packages:upgrade": "yarn upgrade-interactive"
  },
  "repository": {
    "type": "git",
    "url": "https://git.mediamor.de/jumplink.eu/fassbender-shopify-theme"
  },
  "keywords": [
    "JumpLink",
    "Bootstrap4",
    "Shopify"
  ],
  "author": "Pascal Garber <pascal@jumplink.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://git.mediamor.de/jumplink.eu/art-code-studio-shopify-theme/issues"
  },
  "homepage": "https://art-code-studio.myshopify.com",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-export-default-from": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime-corejs3": "^7.9.2",
    "@ribajs/eslint-config": "^1.8.0",
    "@ribajs/tsconfig": "^1.8.0",
    "@ribajs/webpack-config": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "@yarnpkg/pnpify": "^2.0.0-rc.21",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^16.5.1",
    "clean-css-cli": "^4.3.0",
    "concurrently": "^5.1.0",
    "core-js": "^3.6.5",
    "css-flip": "^0.5.0",
    "css-loader": "^3.5.2",
    "csso-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "extract-loader": "^5.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-jsoncombine": "^1.0.4",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^5.0.1",
    "html-loader": "^1.1.0",
    "inspectpack": "^4.4.0",
    "jshint": "^2.11.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.3",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^2.0.0",
    "prettier": "^2.0.4",
    "sass-loader": "^8.0.2",
    "shelljs": "^0.8.3",
    "style-loader": "^1.1.4",
    "terser-webpack-plugin": "^2.3.5",
    "tsify": "^4.0.1",
    "typescript": "^3.8.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1",
    "webpack": "next",
    "webpack-cli": "^3.3.11",
    "webpack-stream": "^5.2.1",
    "yargs": "^15.3.1"
  },
  "dependencies": {
    "@ribajs/bs4": "^1.8.0",
    "@ribajs/core": "^1.8.0",
    "@ribajs/extras": "^1.8.0",
    "@ribajs/i18n": "^1.8.0",
    "@ribajs/jquery": "^1.8.0",
    "@ribajs/router": "^1.8.0",
    "@ribajs/shopify": "^1.8.0",
    "@ribajs/shopify-tda": "^1.8.0",
    "@types/debug": "^4.1.5",
    "@types/jquery": "^3.3.35",
    "@types/p-queue": "^3.2.1",
    "bootstrap": "^4.4.1",
    "debug": "^4.1.1",
    "p-queue": "^6.3.0",
    "popper.js": "^1.16.1"
  },
  "resolutions": {
    "@ribajs/bs4": "portal:../riba/packages/bs4",
    "@ribajs/core": "portal:../riba/packages/core",
    "@ribajs/extras": "portal:../riba/packages/extras",
    "@ribajs/i18n": "portal:../riba/packages/i18n",
    "@ribajs/iconset": "portal:../riba/packages/iconset",
    "@ribajs/jquery": "portal:../riba/packages/jquery",
    "@ribajs/moment": "portal:../riba/packages/moment",
    "@ribajs/pdf": "portal:../riba/packages/pdf",
    "@ribajs/photoswipe": "portal:../riba/packages/photoswipe",
    "@ribajs/router": "portal:../riba/packages/router",
    "@ribajs/shopify": "portal:../riba/packages/shopify",
    "@ribajs/shopify-easdk": "portal:../riba/packages/shopify-easdk",
    "@ribajs/shopify-tda": "portal:../riba/packages/shopify-tda",
    "@ribajs/eslint-config": "portal:../riba/infra/eslint-config",
    "@ribajs/types": "portal:../riba/infra/types",
    "@ribajs/webpack-config": "portal:../riba/infra/webpack-config",
    "@ribajs/tsconfig": "portal:../riba/infra/tsconfig"
  }
}
