{
  "name": "fassbender-shopify-theme",
  "version": "2.7.2",
  "description": "Shopify theme based on Bootstrap 4",
  "main": "src/ts/main.ts",
  "typings": "src/ts/types",
  "scripts": {
    "theme:download": "theme download --dir './theme'",
    "theme:download:configs": "theme download --dir './theme' locales config/settings_data.json",
    "theme:download:locales": "theme download --dir './theme' locales",
    "theme:download:config": "theme download --dir './theme' config/settings_data.json",
    "webpack": "webpack --env production",
    "webpack:dev": "webpack --env development",
    "deploy": "npm run build && theme deploy --dir ./theme",
    "deploy:webpack": "npm run build && theme deploy --dir ./theme assets/main.bundle.js assets/vendors.bundle.js assets/main.css",
    "lint": "eslint ./src --ext .ts,.tsx --fix && tsc --noEmit",
    "test": "npm run lint",
    "build": "npm run lint && npm run webpack",
    "manual:build:prod:sass": "node-sass --output-style compressed src/scss/main.scss -o theme/assets/ && postcss -c postcss.config.js theme/assets/main.css -o theme/assets/main.css",
    "build:dev": "npm run lint && npm run webpack:dev",
    "zip": "gulp build:zip",
    "watch": "concurrently \"npm:watch:dev*\"",
    "watch:dev:theme": "theme watch --dir './theme'",
    "watch:dev:webpack": "npm run webpack:dev -- --watch",
    "watch:prod": "concurrently \"npm:watch:prod:*\"",
    "watch:prod:theme": "theme watch --dir './theme'",
    "watch:prod:webpack": "npm run webpack -- --watch",
    "checkout": "concurrently -k \"npm:checkout:*\"",
    "checkout:js": "uglifyjs checkout/custom-checkout.js -o checkout/custom-checkout.min.js -c -m;",
    "checkout:css": "cleancss checkout/custom-checkout.css -o checkout/custom-checkout.min.css",
    "packages:npm:link": "npm link @ribajs/core @ribajs/i18n @ribajs/extras @ribajs/jquery @ribajs/bs4 @ribajs/shopify @ribajs/router @ribajs/shopify-tda",
    "packages:upgrade": "yarn upgrade-interactive"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ArtCodeStudio/fassbender-shopify-theme.git"
  },
  "keywords": [
    "Art+Code Studio",
    "Bootstrap4",
    "Shopify",
    "myfassbender",
    "Fassbender"
  ],
  "author": "Pascal Garber <pascal@jumplink.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArtCodeStudio/fassbender-shopify-theme/issues"
  },
  "homepage": "https://art-code-studio.myshopify.com",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-syntax-export-default-from": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@babel/runtime-corejs3": "^7.12.1",
    "@ribajs/eslint-config": "^1.9.0-beta.2",
    "@ribajs/tsconfig": "^1.9.0-beta.2",
    "@ribajs/webpack-config": "^1.9.0-beta.2",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@yarnpkg/pnpify": "^2.3.3",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "clean-css-cli": "^4.3.0",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "css-flip": "^0.5.0",
    "css-loader": "^5.0.0",
    "csso-cli": "^3.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "extract-loader": "^5.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.1.1",
    "gulp": "^4.0.2",
    "gulp-jsoncombine": "^1.0.4",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^5.0.2",
    "html-loader": "^1.3.2",
    "inspectpack": "^4.5.2",
    "jshint": "^2.12.0",
    "mini-css-extract-plugin": "^1.1.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.6",
    "postcss": "^8.1.2",
    "postcss-cli": "^8.1.0",
    "postcss-loader": "^4.0.4",
    "postcss-modules": "^3.2.2",
    "prettier": "^2.1.2",
    "sass-loader": "^10.0.3",
    "shelljs": "^0.8.4",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.0",
    "tsify": "^5.0.2",
    "typescript": "^4.0.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0",
    "webpack-stream": "^6.1.0",
    "yargs": "^16.1.0"
  },
  "dependencies": {
    "@ribajs/bs4": "^1.9.0-beta.2",
    "@ribajs/core": "^1.9.0-beta.2",
    "@ribajs/extras": "^1.9.0-beta.2",
    "@ribajs/i18n": "^1.9.0-beta.2",
    "@ribajs/jquery": "^1.9.0-beta.2",
    "@ribajs/router": "^1.9.0-beta.2",
    "@ribajs/shopify": "^1.9.0-beta.2",
    "@ribajs/shopify-tda": "^1.9.0-beta.2",
    "@ribajs/utils": "^1.9.0-beta.2",
    "@types/debug": "^4.1.5",
    "@types/jquery": "^3.5.3",
    "@types/p-queue": "^3.2.1",
    "bootstrap": "^4.5.3",
    "debug": "^4.3.0",
    "p-queue": "^6.6.2",
    "popper.js": "^1.16.1"
  },
  "resolutions": {
    "@ribajs/bs4": "portal:../riba/packages/bs4",
    "@ribajs/core": "portal:../riba/packages/core",
