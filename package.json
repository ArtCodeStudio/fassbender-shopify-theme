{
  "name": "fassbender-shopify-theme",
  "version": "2.0.2",
  "description": "Shopify theme based on Bootstrap 4",
  "main": "src/ts/main.ts",
  "typings": "src/ts/types",
  "scripts": {
    "theme:download": "theme download --dir './theme'",
    "webpack": "webpack --env.production",
    "webpack:dev": "webpack --env.development",
    "deploy": "npm run build && theme deploy --dir ./theme",
    "deploy:ts": "npm run build:prod:ts && theme deploy --dir ./theme assets/main.bundle.js assets/vendors.main.bundle.js",
    "deploy:sass": "npm run build:prod:sass && theme deploy --dir ./theme assets/bundle.css",
    "test:ts": "tslint -c tslint.json 'src/**/*.ts' && tsc --noEmit",
    "test": "npm run test:ts",
    "build": "concurrently \"npm:build:prod:*\"",
    "build:prod:ts": "npm run test:ts && npm run webpack",
    "build:prod:sass": "node-sass --output-style compressed src/scss/bundle.scss -o theme/assets/ && postcss -c postcss.config.js theme/assets/bundle.css -o theme/assets/bundle.css",
    "build:dev": "concurrently \"npm:build:dev:*\"",
    "build:dev:ts": "npm run test:ts && npm run webpack:dev",
    "build:dev:sass": "npm run build:prod:sass",
    "zip": "gulp build:zip",
    "watch": "concurrently \"npm:watch:*\"",
    "watch:theme": "theme watch --dir './theme'",
    "watch:ts": "npm run webpack:dev -- --watch",
    "watch:sass": "nodemon -e scss -x 'npm run build:prod:sass'",
    "checkout": "concurrently -k \"npm:checkout:*\"",
    "checkout:js": "uglifyjs checkout/custom-checkout.js -o checkout/custom-checkout.min.js -c -m;",
    "checkout:css": "cleancss checkout/custom-checkout.css -o checkout/custom-checkout.min.css",
    "packages:link": "npm link @ribajs/core @ribajs/i18n @ribajs/bs4 @ribajs/shopify @ribajs/router @ribajs/shopify-tda"
  },
  "repository": {
    "type": "git",
    "url": "https://git.mediamor.de/jumplink.eu/fassbender-shopify-theme"
  },
  "keywords": [
    "JumpLink",
    "Bootstrap4",
    "Shopify"
  ],
  "author": "Pascal Garber <pascal@jumplink.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://git.mediamor.de/jumplink.eu/art-code-studio-shopify-theme/issues"
  },
  "homepage": "https://art-code-studio.myshopify.com",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/plugin-transform-parameters": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "autoprefixer": "^9.6.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "clean-css-cli": "^4.3.0",
    "concurrently": "^5.0.0",
    "css-flip": "^0.5.0",
    "css-loader": "^3.2.0",
    "csso-cli": "^2.0.2",
    "extract-loader": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-jsoncombine": "^1.0.4",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^5.0.1",
    "html-loader": "^0.5.5",
    "inspectpack": "^4.2.2",
    "jshint": "^2.10.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3",
    "postcss": "^7.0.18",
    "postcss-cli": "^6.1.3",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^1.4.1",
    "sass-loader": "^8.0.0",
    "shelljs": "^0.8.3",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.2",
    "ts-loader": "^6.2.0",
    "tsify": "^4.0.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-stream": "^5.2.1",
    "yargs": "^14.2.0"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.6.2",
    "@ribajs/core": "^1.2.1",
    "@ribajs/i18n": "^1.2.1",
    "@ribajs/router": "^1.2.1",
    "@ribajs/shopify": "^1.2.1",
    "@ribajs/shopify-tda": "^1.2.1",
    "bootstrap": "^4.3.1",
    "popper.js": "^1.15.0"
  }
}
