    "clear": "rimraf ./theme/assets/*.js ./theme/assets/*.css ./theme/assets/*.tsbuildinfo",
    "build": "yarn lint && yarn build:vite",
    "build:vite": "vite build",
    "watch": "concurrently \"npm:watch:dev*\"",
