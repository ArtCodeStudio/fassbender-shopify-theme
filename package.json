{
  "name": "fassbender-shopify-theme",
  "version": "1.6.4",
  "description": "Shopify theme based on Bootstrap 4",
  "main": "src/ts/main.ts",
  "typings": "src/ts/types",
  "scripts": {
    "copy-modules": "node ./utils/copy_modules.js",
    "theme:download": "theme download --dir './theme'",
    "webpack": "webpack",
    "webpack:dev": "webpack --config webpack.config.dev.js",
    "deploy:ts": "npm run build:ts && theme deploy --dir ./theme assets/main.bundle.js assets/vendors.main.bundle.js",
    "deploy:sass": "npm run build:sass && theme deploy --dir ./theme assets/bundle.css",
    "test:ts": "tslint -c tslint.json 'src/**/*.ts' && tsc --noEmit",
    "test": "npm run test:ts",
    "build": "npm run copy-modules && npm run build:ts && npm run build:sass && npm run build:zip",
    "build:dev": "npm run copy-modules && npm run build:ts:dev && npm run build:sass",
    "build:ts": "npm run test:ts && npm run webpack",
    "build:ts:dev": "npm run test:ts && npm run webpack:dev",
    "build:ts:babel": "babel src/ts/main.ts --extensions '.ts'",
    "build:sass": "node-sass --output-style compressed src/scss/bundle.scss -o theme/assets/ && postcss -c postcss.config.js theme/assets/bundle.css -o theme/assets/bundle.css",
    "build:zip": "gulp zip",
    "_watch": "npm run watch:theme & npm run watch:ts & npm run watch:sass",
    "_watch:dev": "npm run watch:theme & npm run watch:ts:dev & npm run watch:sass",
    "watch:theme": "theme watch --dir './theme'",
    "watch": "npm run copy-modules && npm run test:ts && npm run _watch",
    "watch:ts": "npm run webpack:dev -- --watch",
    "watch:sass": "nodemon -e scss -x 'npm run build:sass'",
    "watch:dev": "npm run copy-modules && npm run test:ts && npm run _watch:dev",
    "checkout:js": "uglifyjs checkout/custom-checkout.js -o checkout/custom-checkout.min.js -c -m;",
    "checkout:css": "cleancss checkout/custom-checkout.css -o checkout/custom-checkout.min.css",
    "checkout": "npm run checkout:js && npm run checkout:css",
    "link": "npm link @ribajs/core @ribajs/i18n @ribajs/shopify @ribajs/router @ribajs/shopify-tda"
  },
  "repository": {
    "type": "git",
    "url": "https://git.mediamor.de/jumplink.eu/fassbender-shopify-theme"
  },
  "keywords": [
    "JumpLink",
    "Bootstrap4",
    "Shopify"
  ],
  "author": "Pascal Garber <pascal@jumplink.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://git.mediamor.de/jumplink.eu/art-code-studio-shopify-theme/issues"
  },
  "homepage": "https://art-code-studio.myshopify.com",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/plugin-transform-parameters": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "clean-css-cli": "^4.3.0",
    "css-flip": "^0.5.0",
    "css-loader": "^3.2.0",
    "csso-cli": "^2.0.2",
    "extract-loader": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-jsoncombine": "^1.0.4",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^5.0.0",
    "html-loader": "^0.5.5",
    "jshint": "^2.10.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3",
    "parcel-bundler": "^1.12.3",
    "postcss": "^7.0.18",
    "postcss-cli": "^6.1.3",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^1.4.1",
    "sass-loader": "^8.0.0",
    "shelljs": "^0.8.3",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.2",
    "ts-loader": "^6.2.0",
    "tsify": "^4.0.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-stream": "^5.2.1",
    "yargs": "^14.0.0"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.6.2",
    "@ribajs/core": "^1.1.5",
    "@ribajs/i18n": "^1.1.5",
    "@ribajs/router": "^1.1.5",
    "@ribajs/shopify": "^1.1.5",
    "@ribajs/shopify-tda": "^1.1.5",
    "@types/debug": "^4.1.5",
    "@types/jquery": "^3.3.31",
    "@types/p-queue": "^3.2.1",
    "bootstrap": "^4.3.1",
    "debug": "^4.1.1",
    "jquery": "^3.4.1",
    "p-queue": "^6.1.1",
    "popper.js": "^1.15.0",
    "rxjs": "^6.5.3"
  }
}
