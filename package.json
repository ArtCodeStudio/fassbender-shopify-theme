{
  "name": "fassbender-shopify-theme",
  "version": "2.7.4",
  "description": "Shopify theme based on Bootstrap 4",
  "main": "src/ts/main.ts",
  "typings": "src/ts/types",
  "scripts": {
    "theme:download": "theme download --dir './theme'",
    "theme:download:configs": "theme download --dir './theme' locales config/settings_data.json",
    "theme:download:locales": "theme download --dir './theme' locales",
    "theme:download:config": "theme download --dir './theme' config/settings_data.json",
    "webpack": "webpack --env production",
    "webpack:dev": "webpack --env development",
    "deploy": "npm run build && theme deploy --dir ./theme",
    "deploy:webpack": "npm run build && theme deploy --dir ./theme assets/main.bundle.js assets/vendors.bundle.js assets/main.css",
    "lint": "eslint ./src --ext .ts,.tsx --fix && tsc --noEmit",
    "test": "npm run lint",
    "build": "npm run lint && npm run webpack",
    "manual:build:prod:sass": "node-sass --output-style compressed src/scss/main.scss -o theme/assets/ && postcss -c postcss.config.js theme/assets/main.css -o theme/assets/main.css",
    "build:dev": "npm run lint && npm run webpack:dev",
    "zip": "gulp build:zip",
    "watch": "concurrently \"npm:watch:dev*\"",
    "watch:dev:theme": "theme watch --dir './theme'",
    "watch:dev:webpack": "npm run webpack:dev -- --watch",
    "watch:prod": "concurrently \"npm:watch:prod:*\"",
    "watch:prod:theme": "theme watch --dir './theme'",
    "watch:prod:webpack": "npm run webpack -- --watch",
    "checkout": "concurrently -k \"npm:checkout:*\"",
    "checkout:js": "uglifyjs checkout/custom-checkout.js -o checkout/custom-checkout.min.js -c -m;",
    "checkout:css": "cleancss checkout/custom-checkout.css -o checkout/custom-checkout.min.css",
    "packages:upgrade": "yarn upgrade-interactive"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ArtCodeStudio/fassbender-shopify-theme.git"
  },
  "keywords": [
    "Art+Code Studio",
    "Bootstrap4",
    "Shopify",
    "myfassbender",
    "Fassbender"
  ],
  "author": "Pascal Garber <pascal@jumplink.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArtCodeStudio/fassbender-shopify-theme/issues"
  },
  "homepage": "https://art-code-studio.myshopify.com",
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-export-default-from": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@babel/runtime-corejs3": "^7.15.4",
    "@ribajs/eslint-config": "workspace:*",
    "@ribajs/tsconfig": "workspace:*",
    "@ribajs/webpack-config": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@yarnpkg/pnpify": "^3.0.1-rc.2",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "clean-css-cli": "^4.3.0",
    "concurrently": "^6.2.1",
    "core-js": "^3.17.2",
    "css-flip": "^0.5.0",
    "css-loader": "^6.2.0",
    "csso-cli": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "extract-loader": "^5.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.1.1",
    "gulp": "^4.0.2",
    "gulp-jsoncombine": "^1.0.4",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^5.0.2",
    "html-loader": "^2.1.2",
    "inspectpack": "^4.5.2",
    "jshint": "^2.12.0",
    "mini-css-extract-plugin": "^2.2.2",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.6",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^6.1.1",
    "postcss-modules": "^3.2.2",
    "prettier": "^2.3.2",
    "sass-loader": "^12.1.0",
    "shelljs": "^0.8.4",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.2.3",
    "tsify": "^5.0.2",
    "typescript": "^4.4.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-stream": "^7.0.0",
    "yargs": "^17.1.1"
  },
  "dependencies": {
    "@ribajs/bs4": "workspace:*",
    "@ribajs/core": "workspace:*",
    "@ribajs/extras": "workspace:*",
    "@ribajs/i18n": "workspace:*",
    "@ribajs/jquery": "workspace:*",
    "@ribajs/router": "workspace:*",
    "@ribajs/shopify": "workspace:*",
    "@ribajs/shopify-tda": "workspace:*",
    "@ribajs/utils": "workspace:*",
    "@types/debug": "^4.1.5",
    "@types/jquery": "^3.5.6",
    "@types/p-queue": "^3.2.1",
    "bootstrap": "^4.6.0",
    "debug": "^4.3.0",
    "p-queue": "^6.6.2",
    "popper.js": "^1.16.1"
  },
  "workspaces": [
    "packages/riba/packages/*",
    "packages/riba/backend/*",
    "packages/riba/infra/*",
    "!packages/riba/infra/cli",
