    "@ribajs/types": "^2.0.0-rc.10",
    "@ribajs/utils": "^2.0.0-rc.10",
    "@types/debug": "^4.1.12",
    "@types/jquery": "^3.5.29",
    "@types/p-queue": "^3.2.1",
    "bootstrap": "^5.3.2",
    "debug": "^4.3.4",
