{
  "presets": [
    "@babel/typescript",
    [
      "@babel/preset-env", {
        "targets": {
          "browsers": ["ie >= 11", "safari >= 10", "chrome >= 52", "edge >= 16", "firefox >= 59"]
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "ignore": [
    "**/custom-elements-es5-adapter.js"
  ]
}