{
  "presets": [
    [
      "@babel/typescript", {
        "allExtensions": true
      }
    ],
    [
      "@babel/preset-env", {
        "targets": {
          "ie": "11",
          "safari": "10",
          "chrome": "52",
          "edge": "16",
          "firefox": "59"
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime", {
        "corejs": 2
      }
    ],
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "ignore": [
    "**/custom-elements-es5-adapter.js"
  ]
}