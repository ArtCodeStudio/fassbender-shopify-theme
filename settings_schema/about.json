          "label": "1:2"
        },
        {
          "value": "ratio-3-2",
          "label": "3:2"
        },
        {
          "value": "ratio-4-3",
          "label": "4:3"
        },
        {
          "value": "ratio-16-9",
          "label": "16:9"
        },
        {
          "value": "ratio-3-4",
          "label": "3:4"
        },
        {
          "value": "ratio-3-10",
          "label": "3:10"
        }
      ],
      "default": "ratio-3-10"
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_accessibility",
      "label": "Accessibility",
      "info": "Enables tabbing and arrow key navigation",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_adaptiveHeight",
      "label": "Adaptive Height",
      "info": "Enables adaptive height for single slide horizontal carousels",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_autoplay",
      "label": "Autoplay",
      "info": "Enables Autoplay",
      "default": false
    },
    {
      "type": "number",
      "id": "about-us_carousel_autoplaySpeed",
      "label": "Autoplay Speed",
      "info": "Autoplay Speed in milliseconds",
      "default": 3000
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_arrows",
      "label": "Arrows",
      "info": "Prev\/Next Arrows",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_centerMode",
      "label": "Center Mode",
      "info": "Enables centered view with partial prev\/next slides. Use with odd numbered slidesToShow counts",
      "default": false
    },
    {
      "type": "text",
      "id": "about-us_carousel_centerPadding",
      "label": "Center Padding",
      "info": "Side padding when in center mode (px or %)",
      "default": "50px"
    },
    {
      "type": "text",
      "id": "about-us_carousel_cssEase",
      "label": "CCS Ease",
      "info": "CSS3 Animation Easing",
      "default": "linear"
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_dots",
      "label": "Dots",
      "info": "Show dot indicators",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_draggable",
      "label": "Draggable",
      "info": "Enable mouse dragging",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_fade",
      "label": "Fade",
      "info": "Enable fade",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_focusOnSelect",
      "label": "Focus On Select",
      "info": "Enable focus on selected element (click)",
      "default": false
    },
    {
      "type": "text",
      "id": "about-us_carousel_easing",
      "label": "Easing",
      "info": "Add easing for jQuery animate. Use with easing libraries or default easing methods",
      "default": "linear"
    },
    {
      "type": "text",
      "id": "about-us_carousel_edgeFriction",
      "label": "edgeFriction",
      "info": "Resistance when swiping edges of non-infinite carousels",
      "default": "0.15"
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_infinite",
      "label": "Infinite",
      "info": "Infinite loop sliding",
      "default": true
    },
    {
      "type": "number",
      "id": "about-us_carousel_initialSlide",
      "label": "Initial Slide",
      "info": "Slide to start on",
      "default": 0
    },
    {
      "type": "text",
      "id": "about-us_carousel_lazyLoad",
      "label": "Lazy Load",
      "info": "Set lazy loading technique. Accepts 'ondemand' or 'progressive'",
      "default": "ondemand"
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_mobileFirst",
      "label": "Mobile First",
      "info": "Responsive settings use mobile first calculation",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_pauseOnFocus",
      "label": "Pause On Focus",
      "info": "Pause Autoplay On Focus",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_pauseOnHover",
      "label": "Pause On Hover",
      "info": "Pause Autoplay On Hover",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_pauseOnDotsHover",
      "label": "Pause On Dots Hover",
      "info": "Pause Autoplay when a dot is hovered",
      "default": false
    },
    {
      "type": "number",
      "id": "about-us_carousel_rows",
      "label": "Rows",
      "info": "Setting this to more than 1 initializes grid mode. Use slidesPerRow to set how many slides should be in each row",
      "default": 1
    },
    {
      "type": "number",
      "id": "about-us_carousel_slidesPerRow",
      "label": "Slides Per Row",
      "info": "With grid mode intialized via the rows option, this sets how many slides are in each grid row",
      "default": 1
    },
    {
      "type": "number",
      "id": "about-us_carousel_slidesToShow",
      "label": "Slides To Show",
      "info": "Number of slides to show",
      "default": 1
    },
    {
      "type": "number",
      "id": "about-us_carousel_slidesToScroll",
      "info": "Number of slides to scroll",
      "label": "Slides To Scroll",
      "default": 1
    },
    {
      "type": "number",
      "id": "about-us_carousel_speed",
      "info": "Slide\/Fade animation speed",
      "label": "Speed",
      "default": 300
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_swipe",
      "label": "Swipe",
      "info": "Enable swiping",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_swipeToSlide",
      "label": "Swipe To Slide",
      "info": "Allow users to drag or swipe directly to a slide irrespective of slidesToScroll",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_touchMove",
      "label": "Touch Move",
      "info": "Enable slide motion with touch",
      "default": true
    },
    {
      "type": "number",
      "id": "about-us_carousel_touchThreshold",
      "label": "Touch Threshold",
      "info": "To advance slides, the user must swipe a length of (1\/touchThreshold) * the width of the slider",
      "default": 5
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_useCSS",
      "label": "Use CSS",
      "info": "Enable\/Disable CSS Transitions",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_useTransform",
      "label": "Use Transform",
      "info": "Enable\/Disable CSS Transforms",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_variableWidth",
      "label": "Variable Width",
      "info": "Variable width slides",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_vertical",
      "label": "Vertical",
      "info": "Vertical slide mode",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_verticalSwiping",
      "label": "verticalSwiping",
      "info": "Vertical swipe mode",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_rtl",
      "label": "right-to-left",
      "info": "Change the slider's direction to become right-to-left",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "about-us_carousel_waitForAnimate",
      "label": "Wait For Animate",
      "info": "Ignores requests to advance the slide while animating",
      "default": false
    }
